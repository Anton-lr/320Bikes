<urn:baqme_2_hubs> a rr:TriplesMap;
  rr:predicateObjectMap [ a rr:PredicateObjectMap;
      rr:predicate :name;
      rr:objectMap [ a rr:TermMap, rr:ObjectMap;
          rr:column "name";
          rr:termType rr:Literal
        ]
    ], [ a rr:PredicateObjectMap;
      rr:predicate :ownedBy;
      rr:objectMap [ a rr:TermMap, rr:ObjectMap;
          rr:column "company_name";
          rr:termType rr:Literal
        ]
    ];
  rr:logicalTable [ a rr:R2RMLView;
      rr:sqlQuery "select * from \"320bikes\".baqme_2, \"320bikes\".data_overview where short_name = 'baqme_2'"
    ];
  rr:subjectMap [ a rr:TermMap, rr:SubjectMap;
      rr:template "http://www.example.org/bikeOntology#320bikes/baqme_2{station_id}";
      rr:termType rr:IRI;
      rr:class :hub
    ] .

