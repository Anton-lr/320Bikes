source		select * from blue_4

mappingId	donkey2
target		:hub/donkey_2/{station_id} a :Hub ; :hasAddress {name} ; :geopoint {geopunt} ; :hasLongitude {lon}^^xsd:float ; :hasLatitude {lat}^^xsd:float ; :bikesAvailable {num_bikes_available} ; :ownedBy :company/{company} ; :vehicleType {vehicle_type} . 
source		select * from donkey_2

