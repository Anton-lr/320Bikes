mappingId	baqme_bikes
target		:vehicle/baqme_bikes/{bike_id} a :ElectricCargoBike ; :geopoint {geopoint} ; :hasLatitude {lat}^^xsd:float ; :hasLongitude {lon}^^xsd:float ; :ownedBy :company/{company} ; :vehicleType {vehicle_type} . 
