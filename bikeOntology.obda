[PrefixDeclaration]
:		http://www.example.org/bikeOntology#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
xml:		http://www.w3.org/XML/1998/namespace
xsd:		http://www.w3.org/2001/XMLSchema#
obda:		https://w3id.org/obda/vocabulary#
rdfs:		http://www.w3.org/2000/01/rdf-schema#

[MappingDeclaration] @collection [[
mappingId	baqme1
target		:hub/baqme_1/{station_id} a :Hub ; :hasAddress {name} ; :hasLongitude {lon}^^xsd:float ; :hasLatitude {lat}^^xsd:float ; :geopoint {geopunt} ; :bikesAvailable {num_bikes_available} ; :ownedBy :company/{company} ; :vehicleType {vehicle_type} . 
source		select * from baqme_1

mappingId	baqme2
target		:hub/baqme_2/{station_id} a :Hub ; :hasAddress {name} ; :hasLongitude {lon}^^xsd:float ; :hasLatitude {lat}^^xsd:float ; :geopoint {geopunt} ; :ownedBy :company/{company} ; :vehicleType {vehicle_type} . 
