[PrefixDeclaration]
:		http://www.example.org/bikeOntology#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
xml:		http://www.w3.org/XML/1998/namespace
xsd:		http://www.w3.org/2001/XMLSchema#
rdfs:		http://www.w3.org/2000/01/rdf-schema#

[MappingDeclaration] @collection [[
mappingId	blue1 mapping
target		:ghentbikes/blue1/{id} a :Hub ; :hasAddress {name} ; :hasLatitude {latitude} ; :hasLongitude {longitude} ; :ownedBy {company_name} . 
source		select * from ghentbikes.blue1, ghentbikes.dataoverview where short_name = 'blue_1'

mappingId	blue2 mapping
target		:ghentbikes/blue2/{id} a :Hub . 
source		select * from ghentbikes.blue2
]]

