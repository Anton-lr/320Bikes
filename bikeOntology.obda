[PrefixDeclaration]
:		http://www.example.org/bikeOntology#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
xml:		http://www.w3.org/XML/1998/namespace
xsd:		http://www.w3.org/2001/XMLSchema#
obda:		https://w3id.org/obda/vocabulary#
rdfs:		http://www.w3.org/2000/01/rdf-schema#

[MappingDeclaration] @collection [[
mappingId	companymapping
target		:ghentbikes/dataoverview{company_name} a :Company ; :name {company_name} . 
source		SELECT * FROM ghentbikes.dataoverview

mappingId	donkeyhub
target		:ghentbikes/donkey2{station_id} a :Hub ; :geopoint {company_name} ; :ownedBy :ghentbikes/dataoverview{company_name} ; :fromSource :ghentbikes/dataoverview{short_name} ; :bikesAvailable {num_bikes_available} . 
source		SELECT * FROM ghentbikes.donkey2, ghentbikes.dataoverview where short_name = 'donkey_2'

mappingId	datasourcemapping
target		:ghentbikes/dataoverview{short_name} a :DataSource ; :full_name {full_name} ; :producedBy :ghentbikes/dataoverview{company_name} ; :link {link} . 
source		SELECT * FROM ghentbikes.dataoverview
]]

