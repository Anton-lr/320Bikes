@prefix : <http://www.example.org/bikeOntology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<urn:companymapping> a rr:TriplesMap;
  rr:logicalTable [ a rr:R2RMLView;
      rr:sqlQuery "SELECT * FROM ghentbikes.dataoverview"
    ];
  rr:predicateObjectMap [ a rr:PredicateObjectMap;
      rr:objectMap [ a rr:ObjectMap, rr:TermMap;
          rr:column "company_name";
          rr:termType rr:Literal
        ];
      rr:predicate :name
    ];
  rr:subjectMap [ a rr:SubjectMap, rr:TermMap;
      rr:class :Company;
      rr:template "http://www.example.org/bikeOntology#ghentbikes/dataoverview{company_name}";
      rr:termType rr:IRI
    ] .

<urn:donkeyhub> a rr:TriplesMap;
  rr:logicalTable [ a rr:R2RMLView;
      rr:sqlQuery "SELECT * FROM ghentbikes.donkey2, ghentbikes.dataoverview where short_name = 'donkey_2'"
    ];
  rr:predicateObjectMap [ a rr:PredicateObjectMap;
      rr:objectMap [ a rr:ObjectMap, rr:TermMap;
          rr:column "company_name";
          rr:termType rr:Literal
        ];
      rr:predicate :geopoint
    ], [ a rr:PredicateObjectMap;
      rr:objectMap [ a rr:ObjectMap, rr:TermMap;
          rr:template "http://www.example.org/bikeOntology#ghentbikes/dataoverview{company_name}";
          rr:termType rr:IRI
        ];
      rr:predicate :ownedBy
    ], [ a rr:PredicateObjectMap;
      rr:objectMap [ a rr:ObjectMap, rr:TermMap;
          rr:template "http://www.example.org/bikeOntology#ghentbikes/dataoverview{short_name}";
          rr:termType rr:IRI
        ];
      rr:predicate :fromSource
    ], [ a rr:PredicateObjectMap;
      rr:objectMap [ a rr:ObjectMap, rr:TermMap;
          rr:column "num_bikes_available";
          rr:termType rr:Literal
        ];
      rr:predicate :bikesAvailable
    ];
  rr:subjectMap [ a rr:SubjectMap, rr:TermMap;
      rr:class :Hub;
      rr:template "http://www.example.org/bikeOntology#ghentbikes/donkey2{station_id}";
      rr:termType rr:IRI
    ] .

<urn:datasourcemapping> a rr:TriplesMap;
  rr:logicalTable [ a rr:R2RMLView;
      rr:sqlQuery "SELECT * FROM ghentbikes.dataoverview"
    ];
  rr:predicateObjectMap [ a rr:PredicateObjectMap;
      rr:objectMap [ a rr:ObjectMap, rr:TermMap;
          rr:column "full_name";
          rr:termType rr:Literal
        ];
      rr:predicate :full_name
    ], [ a rr:PredicateObjectMap;
      rr:objectMap [ a rr:ObjectMap, rr:TermMap;
          rr:template "http://www.example.org/bikeOntology#ghentbikes/dataoverview{company_name}";
          rr:termType rr:IRI
        ];
      rr:predicate :producedBy
    ], [ a rr:PredicateObjectMap;
      rr:objectMap [ a rr:ObjectMap, rr:TermMap;
          rr:column "link";
          rr:termType rr:Literal
        ];
      rr:predicate :link
    ];
  rr:subjectMap [ a rr:SubjectMap, rr:TermMap;
      rr:class :DataSource;
      rr:template "http://www.example.org/bikeOntology#ghentbikes/dataoverview{short_name}";
      rr:termType rr:IRI
    ] .
